<div class="plugin_maker_code">
    {% if Product.Maker is not empty %}
        {% if Product.maker_url is not empty %}
            <p class="item_code">{{ 'admin.plugin.maker.maker'|trans }}： <a href="{{ Maker.url }}" target="_blank">{{ Maker.name }}</a></p>
        {% else %}
            <p class="item_code">{{ 'admin.plugin.maker.maker'|trans }}： {{ Maker.name }}</p>
        {% endif %}
    {% endif %}
</div>
