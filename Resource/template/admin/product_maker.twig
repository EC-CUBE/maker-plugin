{%- extends '@admin/Form/bootstrap_4_horizontal_layout.html.twig' -%}

{%- block _admin_product_Maker_row -%}
    <div class="row">
        <div class="col-3">
            <div class="d-inline-block">
                <span>
                    {{ form.vars.label|trans }}
                </span>
            </div>
        </div>
        <div class="col mb-2">
            {{- form_widget(form) -}}
            {{- form_errors(form) -}}
        </div>
    </div>
{%- endblock _admin_product_Maker_row -%}

